<body>
  <div class="container">
    <form action="/generate" method="POST">
    <div class="row">
     
      <div class="col-4 infor">
        <h2 class="text-center"><b>Baby full Name</b></h2>
        <input type="text" name="name" class="form-control" placeholder="Write Baby name here">
      </div>
      </div>

     
      <div class="row info">
        <div class="col-4">
          <h5 class="text-center">Date</h5>
          <input type="date" name="date" class="form-control" placeholder="Date">
        </div>
     
        <div class="col-4">
          <h5 class="text-center">Weight</h5>
          <input type="text" name="weight" class="form-control" placeholder="Weight">
        </div>
        <div class="col-4">
          <h5 class="text-center">Height</h5>
          <input type="text" name="height" class="form-control" placeholder="Height">
        </div>
   
    
    <input type="submit" class="btn btn-lg btn-outline-info font-weight-bold info" value="Generate">
  </form>  
  </form>
      </div>
    </div>



